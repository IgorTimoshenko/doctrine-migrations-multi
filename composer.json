        {
            "name": "Igor Timoshenko",
            "email": "igor.timoshenko@i.ua",
            "homepage": "http://igortimoshenko.com"
        }
    ],
    "support": {
        "email": "igor.timoshenko@i.ua",
        "issues": "http://github.com/IgorTimoshenko/doctrine-migrations-multi/issues",
        "wiki": "http://github.com/IgorTimoshenko/doctrine-migrations-multi/wiki",
        "source": "http://github.com/IgorTimoshenko/doctrine-migrations-multi"
    },
    "require": {
        "php": "^5.5|^7.0",
        "doctrine/migrations": "~1.0",
        "symfony/event-dispatcher": "~2.3"
    },
    "require-dev": {
        "phploc/phploc": "2.*",
